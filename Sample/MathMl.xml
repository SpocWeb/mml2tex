<mml:math xmlns:mml="http://www.w3.org/1998/Math/MathML" mode="display" xmlns:m="http://schemas.openxmlformats.org/officeDocument/2006/math">
 <semantics>
  <mml:mrow>
    <mml:mi>x</mml:mi>
    <mml:mo>=</mml:mo>
    <mml:mfrac>
      <mml:mrow>
        <mml:mo form="prefix">&#x2212;<!-- − --></mml:mo>
        <mml:mi>b</mml:mi>
        <mml:mo>&#x00B1;<!-- &PlusMinus; --></mml:mo>
        <mml:msqrt>
          <mml:msup>
            <mml:mi>b</mml:mi>
            <mml:mn>2</mml:mn>
          </mml:msup>
          <mml:mo>&#x2212;<!-- − --></mml:mo>
          <mml:mn>4</mml:mn>
          <mml:mo>&#x2062;<!-- &InvisibleTimes; --></mml:mo>
          <mml:mi>a</mml:mi>
          <mml:mo>&#x2062;<!-- &InvisibleTimes; --></mml:mo>
          <mml:mi>c</mml:mi>
        </mml:msqrt>
      </mml:mrow>
      <mml:mrow>
        <mml:mn>2</mml:mn>
        <mml:mo>&#x2062;<!-- &InvisibleTimes; --></mml:mo>
        <mml:mi>a</mml:mi>
      </mml:mrow>
    </mml:mfrac>
  </mml:mrow>
  <annotation encoding="TeX">
     x=\frac{-b\pm\sqrt{b^2-4ac}}{2a}
  </annotation>
  <annotation encoding="AsciiMath">
     x=(-b +- sqrt(b^2 – 4ac))/(2a)
  </annotation>
  <annotation encoding="StarMath 5.0">
     x={-b plusminus sqrt {b^2 - 4 ac}} over {2 a}
  </annotation>
 </semantics>
</mml:math>